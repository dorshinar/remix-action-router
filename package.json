{
  "name": "remix-action-router",
  "version": "0.0.5",
  "description": "Declarative action matching for Remix actions",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "scripts": {
    "test": "vitest",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "build": "tsup",
    "release": "npm run build && changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dorshinar/remix-action-router.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dorshinar/remix-action-router/issues"
  },
  "homepage": "https://github.com/dorshinar/remix-action-router#readme",
  "dependencies": {
    "tiny-invariant": "^1.2.0"
  },
  "peerDependencies": {
    "@remix-run/node": "^1.6.4",
    "@remix-run/react": "^1.6.4",
    "@remix-run/serve": "^1.6.4",
    "@remix-run/server-runtime": "^1.6.4",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.23.2",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "prettier": "^2.7.1",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "vitest": "^0.18.0"
  }
}
